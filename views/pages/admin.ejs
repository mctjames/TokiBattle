<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>All Trainers</title>

    <link rel="stylesheet" type="text/css" href="/stylesheets/admin.css" />
    <script src="js/script.js"></script>
  </head>
  <body>
    <div id="backgroundImage">
        <div id="header">
            <h1>
                All Trainers Using TokiBattle
            </h1>
        </div>
      <div>
        <table>
          <thead>
            <tr>
              <th>
                Trainers
              </th>
            </tr>
          </thead>
          <tbody>
            <% rows.forEach((r) => { %>
              <tr>
                <td>
                    <%= r.username %>
                </td>
              </tr> 
            <% }); %>
          </tbody>
        </table>
      </div>
    </div>
  </body>
</html>